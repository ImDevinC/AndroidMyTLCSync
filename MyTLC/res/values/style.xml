        <item name="android:actionOverflowButtonStyle">@style/Widget.Sherlock.BlueWhite.ActionButton.Overflow</item>
        <item name="actionOverflowButtonStyle">@style/Widget.Sherlock.BlueWhite.ActionButton.Overflow</item>
    </style>
